{
  "name": "basic-styled-uikit",
  "version": "0.7.1",
  "description": "A set of basic react UIkit-inspired components built with styled-components",
  "keywords": [
    "react",
    "redux",
    "styled-components"
  ],
  "homepage": "https://github.com/thegrinder/basic-styled-uikit",
  "repository": {
    "type": "git",
    "url": "https://github.com/thegrinder/basic-styled-uikit.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js",
    "production": "rm -rf lib && webpack --config config/webpack.prod.js",
    "prepublishOnly": "npm run production",
    "lint": "eslint --fix src/. config/.",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "Pawel Szczepkowski",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-export-namespace-from": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "^5.11.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "react-styleguidist": "^8.0.6",
    "styled-components": "^4.1.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "peerDependencies": {
    "styled-components": "^4.1.3"
  },
  "dependencies": {
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "webpack": "^4.28.2"
  }
}
